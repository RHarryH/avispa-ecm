{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "checkbox"
      ]
    },
    "label": {
      "type": "string"
    },
    "property": {
      "type": "string",
      "pattern": "^[a-zA-Z](?:[a-zA-Z\\d.]*[a-zA-Z\\d])?+$"
    },
    "customValidation": {
      "$ref": "../property/custom-validation.json#"
    }
  },
  "required": [
    "type",
    "property"
  ]
}