{
  "additionalProperties": false,
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "table"
      ]
    },
    "label": {
      "type": "string"
    },
    "property": {
      "type": "string",
      "pattern": "^[A-Za-z][A-Za-z0-9.]*$"
    },
    "propertyType": {
      "type": "string"
    },
    "controls": {
      "type": "array",
      "items": {
        "type": "object",
        "minItem": 1,
        "oneOf": [
          { "$ref": "combo.json#" },
          { "$ref": "date.json#" },
          { "$ref": "datetime.json#" },
          { "$ref": "money.json#" },
          { "$ref": "number.json#" },
          { "$ref": "text.json#" }
        ]
      }
    }
  },
  "required": [
    "type",
    "label",
    "property",
    "propertyType",
    "controls"
  ]
}