{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "function": {
      "type": "string",
      "pattern": "^[_$a-zA-Z][_$a-zA-Z0-9]+$"
    },
    "message": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9 !?]+$"
    }
  }
}