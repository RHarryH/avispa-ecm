{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "additionalProperties": false,
  "description": "Schema for autoname configuration",
  "type": "object",
  "properties": {
    "autoname": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1
        },
        "rule": {
          "type": "string",
          "minLength": 1
        },
        "propertyName": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "name",
        "rule",
        "propertyName"
      ]
    }
  },
  "required": [
    "autoname"
  ]
}